{
  "name": "@elizaos/plugin-rss",
  "description": "RSS and Atom feed integration plugin for elizaOS - enables agents to fetch and monitor news feeds",
  "version": "2.0.0-alpha.2",
  "type": "module",
  "main": "dist/node/index.node.js",
  "module": "dist/node/index.node.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "author": "elizaOS Team",
  "keywords": [
    "plugin",
    "elizaos",
    "rss",
    "atom",
    "feed",
    "news",
    "syndication"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos/eliza"
  },
  "homepage": "https://elizaos.ai",
  "bugs": {
    "url": "https://github.com/elizaos/eliza/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "types": "./dist/node/index.d.ts",
        "import": "./dist/node/index.node.js",
        "default": "./dist/node/index.node.js"
      },
      "bun": {
        "types": "./dist/node/index.d.ts",
        "default": "./dist/node/index.node.js"
      },
      "default": "./dist/node/index.node.js"
    }
  },
  "files": [
    "dist",
    "rust",
    "README.md"
  ],
  "dependencies": {
    "@elizaos/core": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "^1.3.5",
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build:ts": "bun run build.ts",
    "dev": "bun run build.ts --watch",
    "clean": "rm -rf dist   .turbo node_modules",
    "format": "bunx @biomejs/biome format --write .",
    "format:check": "bunx @biomejs/biome format .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:ts": "vitest run",
    "lint": "bunx @biomejs/biome check --write --unsafe .",
    "lint:check": "bunx @biomejs/biome check .",
    "build": "bun run build.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "RSS_FEEDS": {
        "type": "string",
        "description": "JSON array or comma-separated list of feed URLs to auto-subscribe",
        "required": false,
        "sensitive": false
      },
      "RSS_DISABLE_ACTIONS": {
        "type": "boolean",
        "description": "Set to true to disable subscription management actions",
        "required": false,
        "default": false,
        "sensitive": false
      },
      "RSS_FEED_FORMAT": {
        "type": "string",
        "description": "Output format for feed items: 'csv' (compact) or 'markdown' (readable)",
        "required": false,
        "default": "csv",
        "sensitive": false
      },
      "RSS_CHECK_INTERVAL_MINUTES": {
        "type": "number",
        "description": "Interval in minutes between feed checks",
        "required": false,
        "default": 15,
        "sensitive": false
      }
    },
    "description": "RSS and Atom feed integration plugin for elizaOS"
  }
}
