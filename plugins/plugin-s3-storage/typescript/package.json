{
  "name": "@elizaos/plugin-s3-storage",
  "version": "2.0.0-alpha.2",
  "type": "module",
  "main": "dist/cjs/index.node.cjs",
  "module": "dist/node/index.node.js",
  "types": "dist/index.d.ts",
  "browser": "dist/browser/index.browser.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos-plugins/plugin-storage-s3.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "browser": {
        "types": "./dist/browser/index.d.ts",
        "import": "./dist/browser/index.browser.js",
        "default": "./dist/browser/index.browser.js"
      },
      "node": {
        "types": "./dist/node/index.d.ts",
        "import": "./dist/node/index.node.js",
        "require": "./dist/cjs/index.node.cjs",
        "default": "./dist/node/index.node.js"
      },
      "default": "./dist/node/index.node.js"
    }
  },
  "files": [
    "dist",
    "rust"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/s3-request-presigner": "^3.705.0",
    "@elizaos/core": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build:ts": "bun run build.ts",
    "dev": "bun --hot build.ts",
    "lint": "bunx @biomejs/biome check --write --unsafe .",
    "lint:check": "bunx @biomejs/biome check .",
    "clean": "rm -rf dist   .turbo node_modules",
    "format": "bunx @biomejs/biome format --write .",
    "format:check": "bunx @biomejs/biome format .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:ts": "vitest run",
    "build": "bun run build.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "AWS_ACCESS_KEY_ID": {
        "type": "string",
        "description": "AWS access key ID used for authenticating requests to AWS S3.",
        "required": true,
        "sensitive": true
      },
      "AWS_SECRET_ACCESS_KEY": {
        "type": "string",
        "description": "AWS secret access key used for authenticating requests to AWS S3.",
        "required": true,
        "sensitive": true
      },
      "AWS_REGION": {
        "type": "string",
        "description": "AWS region in which the S3 bucket resides.",
        "required": true,
        "sensitive": false
      },
      "AWS_S3_BUCKET": {
        "type": "string",
        "description": "Name of the AWS S3 bucket used for file uploads and downloads.",
        "required": true,
        "sensitive": false
      },
      "AWS_S3_UPLOAD_PATH": {
        "type": "string",
        "description": "Default path or prefix inside the S3 bucket where files will be uploaded.",
        "required": false,
        "sensitive": false
      },
      "AWS_S3_ENDPOINT": {
        "type": "string",
        "description": "Custom S3-compatible endpoint (e.g., for non-AWS providers).",
        "required": false,
        "sensitive": false
      },
      "AWS_S3_SSL_ENABLED": {
        "type": "boolean",
        "description": "Flag indicating whether SSL should be enabled for the custom S3 endpoint.",
        "required": false,
        "sensitive": false
      },
      "AWS_S3_FORCE_PATH_STYLE": {
        "type": "boolean",
        "description": "Flag to force path-style addressing when connecting to the S3 endpoint.",
        "required": false,
        "sensitive": false
      }
    }
  }
}
