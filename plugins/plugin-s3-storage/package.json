{
  "name": "@elizaos/plugin-s3-storage",
  "version": "1.0.3",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos-plugins/plugin-storage-s3.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "scripts",
    "package.json",
    "LICENSE",
    "tsup.config.ts"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/s3-request-presigner": "^3.705.0",
    "@elizaos/core": "^1.0.0",
    "@types/uuid": "10.0.0",
    "capsolver-npm": "2.0.2",
    "fluent-ffmpeg": "2.1.3",
    "glob": "11.0.0",
    "uuid": "11.0.3"
  },
  "devDependencies": {
    "@types/node": "22.8.4",
    "@vitest/coverage-v8": "1.1.3",
    "prettier": "3.5.3",
    "tsup": "8.4.0",
    "typescript": "^5.8.2",
    "vitest": "1.6.1",
    "zod": "^3.22.4"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "postinstall": "node scripts/postinstall.js",
    "lint": "prettier --write ./src",
    "clean": "rm -rf dist .turbo node_modules .turbo-tsconfig.json tsconfig.tsbuildinfo",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "whatwg-url": "7.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "646c632924826e2b75c2304a75ee56959fe4a460",
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "AWS_ACCESS_KEY_ID": {
        "type": "string",
        "description": "AWS access key ID used for authenticating requests to AWS S3.",
        "required": true,
        "sensitive": true
      },
      "AWS_SECRET_ACCESS_KEY": {
        "type": "string",
        "description": "AWS secret access key used for authenticating requests to AWS S3.",
        "required": true,
        "sensitive": true
      },
      "AWS_REGION": {
        "type": "string",
        "description": "AWS region in which the S3 bucket resides.",
        "required": true,
        "sensitive": false
      },
      "AWS_S3_BUCKET": {
        "type": "string",
        "description": "Name of the AWS S3 bucket used for file uploads and downloads.",
        "required": true,
        "sensitive": false
      },
      "AWS_S3_UPLOAD_PATH": {
        "type": "string",
        "description": "Default path or prefix inside the S3 bucket where files will be uploaded.",
        "required": false,
        "sensitive": false
      },
      "AWS_S3_ENDPOINT": {
        "type": "string",
        "description": "Custom S3-compatible endpoint (e.g., for non-AWS providers).",
        "required": false,
        "sensitive": false
      },
      "AWS_S3_SSL_ENABLED": {
        "type": "boolean",
        "description": "Flag indicating whether SSL should be enabled for the custom S3 endpoint.",
        "required": false,
        "sensitive": false
      },
      "AWS_S3_FORCE_PATH_STYLE": {
        "type": "boolean",
        "description": "Flag to force path-style addressing when connecting to the S3 endpoint.",
        "required": false,
        "sensitive": false
      }
    }
  }
}
