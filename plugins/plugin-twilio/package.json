{
  "name": "@elizaos/plugin-twilio",
  "version": "1.0.10",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos-plugins/plugin-twilio.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizaos/core": "workspace:*",
    "twilio": "^5.4.0",
    "express": "^4.18.2",
    "body-parser": "^1.20.2",
    "ws": "^8.16.0",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.5",
    "fluent-ffmpeg": "^2.1.3",
    "node-cache": "^5.1.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/ws": "^8.5.10",
    "@types/fluent-ffmpeg": "^2.1.24",
    "prettier": "3.5.3",
    "tsup": "8.4.0",
    "vitest": "1.6.1"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "elizaos test",
    "test:unit": "vitest",
    "lint": "prettier --write ./src",
    "clean": "rm -rf dist .turbo node_modules .turbo-tsconfig.json tsconfig.tsbuildinfo",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  },
  "peerDependencies": {
    "whatwg-url": "7.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "646c632924826e2b75c2304a75ee56959fe4a460",
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "TWILIO_ACCOUNT_SID": {
        "type": "string",
        "description": "Twilio Account SID used to authenticate with Twilio API",
        "required": true,
        "sensitive": true
      },
      "TWILIO_AUTH_TOKEN": {
        "type": "string",
        "description": "Twilio Auth Token used to authenticate with Twilio API",
        "required": true,
        "sensitive": true
      },
      "TWILIO_PHONE_NUMBER": {
        "type": "string",
        "description": "Twilio phone number to send messages from (E.164 format, e.g., +18885551212)",
        "required": true,
        "sensitive": false
      },
      "TWILIO_WEBHOOK_URL": {
        "type": "string",
        "description": "Public URL for receiving Twilio webhooks (e.g., https://your-domain.com/webhooks/twilio)",
        "required": true,
        "sensitive": false
      },
      "TWILIO_WEBHOOK_PORT": {
        "type": "string",
        "description": "Port to run the webhook server on (default: 3000)",
        "required": false,
        "sensitive": false
      },
      "TWILIO_TEST_PHONE_NUMBER": {
        "type": "string",
        "description": "Phone number to use for testing (E.164 format)",
        "required": false,
        "sensitive": false
      }
    }
  }
}
