{
  "name": "@elizaos/plugin-forms",
  "description": "Forms plugin for ElizaOS - structured conversational data collection",
  "version": "1.2.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageType": "plugin",
  "platform": "node",
  "license": "MIT",
  "author": "elizaOS",
  "keywords": [
    "plugin",
    "elizaos",
    "forms",
    "data-collection",
    "conversational-forms",
    "multi-step-forms"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos/elizaos"
  },
  "homepage": "https://elizaos.ai",
  "bugs": {
    "url": "https://github.com/elizaos/elizaos/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@elizaos/core": "^1.2.0",
    "uuid": "^11.0.2",
    "zod": "^4.0.5"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@elizaos/cli": "^1.2.0",
    "@types/bun": "^1.2.18",
    "@types/uuid": "^10.0.0",
    "dotenv": "^17.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "bun run build.ts",
    "dev": "bun run build.ts --watch",
    "clean": "rm -rf dist && rm -rf .turbo",
    "lint": "eslint src --fix && prettier --write ./src",
    "typecheck": "tsc --noEmit",
    "test": "bun test && elizaos test"
  },
  "publishConfig": {
    "access": "public"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {},
    "description": "Forms management plugin for structured conversational data collection"
  }
}
