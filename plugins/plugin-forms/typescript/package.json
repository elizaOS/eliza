{
  "name": "@elizaos/plugin-forms",
  "version": "1.2.0",
  "description": "Forms plugin for ElizaOS - structured conversational data collection",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos/elizaos"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "plugin",
    "elizaos",
    "forms",
    "data-collection",
    "conversational-forms",
    "multi-step-forms"
  ],
  "author": "elizaOS",
  "license": "MIT",
  "scripts": {
    "build": "bun run build.ts",
    "build:ts": "bun run build.ts",
    "dev": "bun --hot build.ts",
    "clean": "rm -rf dist .turbo node_modules",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "bunx @biomejs/biome check --write .",
    "lint:check": "bunx @biomejs/biome check .",
    "format": "bunx @biomejs/biome format --write .",
    "format:check": "bunx @biomejs/biome format ."
  },
  "dependencies": {
    "@elizaos/core": "workspace:*",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3",
    "@biomejs/biome": "^2.3.11"
  },
  "peerDependencies": {
    "@elizaos/core": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {},
    "description": "Forms management plugin for structured conversational data collection"
  }
}
