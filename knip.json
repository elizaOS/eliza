{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "packages/**/src/index.ts",
    "packages/**/index.ts",
    "plugins/**/typescript/src/index.ts",
    "plugins/**/typescript/index.ts",
    "apps/**/src/index.ts",
    "apps/**/index.ts",
    "!**/examples/**",
    "!**/scripts/**"
  ],
  "project": [
    "packages/**/*.{ts,tsx,js,jsx}",
    "plugins/**/*.{ts,tsx,js,jsx}",
    "apps/**/*.{ts,tsx,js,jsx}",
    "!**/examples/**",
    "!**/scripts/**",
    "!**/*.test.{ts,tsx,js,jsx}",
    "!**/*.spec.{ts,tsx,js,jsx}",
    "!**/__tests__/**",
    "!**/cypress/**",
    "!**/*.config.{ts,js}",
    "!**/build.config.ts",
    "!**/tsup.config.ts",
    "!**/drizzle.config.ts",
    "!**/generated/**",
    "!**/*.d.ts"
  ],
  "ignore": [
    "**/dist/**",
    "**/node_modules/**",
    "**/target/**",
    "**/pkg/**",
    "**/coverage/**",
    "**/.turbo/**",
    "**/.next/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/__tests__/**",
    "**/__mocks__/**",
    "**/tests.ts",
    "**/test-*.ts",
    "**/test-*.js",
    "**/cypress/**",
    "**/vitest.config.ts",
    "**/build.ts",
    "**/build.tsx",
    "**/build.js",
    "**/build.config.ts",
    "**/tsup.config.ts",
    "**/drizzle.config.ts",
    "**/vite.config.ts",
    "**/tailwind.config.js",
    "**/postcss.config.js",
    "**/cypress.config.ts",
    "**/.prettierrc.js",
    "**/tsconfig*.json",
    "**/turbo.json",
    "**/lerna.json",
    "**/package.json",
    "**/bun.lock*",
    "**/package-lock.json",
    "**/yarn.lock",
    "**/pnpm-lock.yaml",
    "**/Cargo.toml",
    "**/Cargo.lock",
    "**/pyproject.toml",
    "**/*.proto",
    "**/*.md",
    "**/*.mdx",
    "**/*.json",
    "**/*.yaml",
    "**/*.yml",
    "**/*.toml",
    "**/*.lock",
    "**/*.wasm",
    "**/*.rs",
    "**/*.py",
    "**/*.sh",
    "**/*.css",
    "**/*.html",
    "**/*.svg",
    "**/*.png",
    "**/*.jpg",
    "**/*.jpeg",
    "**/.git/**",
    "**/.eliza/**",
    "**/.elizadb/**",
    "**/data/**",
    "**/logs/**",
    "**/typescript/typescript/**",
    "**/typescript/typescript/typescript/**",
    "**/examples/**",
    "**/scripts/**",
    "**/postinstall.js",
    "**/preinstall.js",
    "**/run-benchmark*.ts",
    "**/run-realm*.ts",
    "**/generated/**",
    "**/*.d.ts",
    "**/index.browser.ts",
    "**/index.node.ts",
    "**/schema/types.ts",
    "**/utils.node.ts",
    "**/environment.ts",
    "**/bignumber.ts",
    "**/types.d.ts",
    "**/apis.ts",
    "**/schema.ts",
    "**/templates.ts",
    "**/constants.ts",
    "**/benchmarks/**",
    "**/client/index.ts",
    "**/services/index.ts",
    "**/providers/index.ts",
    "**/actions/index.ts",
    "**/utils/index.ts",
    "**/types/index.ts",
    "**/managers/index.ts",
    "**/build-utils.ts",
    "**/packages/interop/**",
    "**/packages/rust/pkg-node/**",
    "**/streaming-context.browser.ts",
    "**/testing/index.ts",
    "**/utils/node.ts",
    "**/actions/*.ts",
    "**/providers/*.ts",
    "**/service.ts",
    "**/attachments.ts",
    "**/banner.ts",
    "**/compat.ts",
    "**/messages.ts",
    "**/permissions.ts",
    "**/utils.ts",
    "**/voice.ts",
    "**/frontend/**",
    "**/stagehand-server/**",
    "**/utils/*.ts",
    "**/managers/*.ts",
    "**/plugin-*/typescript/actions/*.ts",
    "**/plugin-*/typescript/providers/*.ts",
    "**/plugin-*/typescript/service.ts",
    "**/plugin-*/typescript/index.ts",
    "**/plugin-*/typescript/src/index.ts",
    "**/plugin-*/typescript/src/actions/*.ts",
    "**/plugin-*/typescript/src/providers/*.ts",
    "**/plugin-*/typescript/src/service.ts",
    "**/plugin-*/typescript/src/services/*.ts",
    "**/plugin-*/typescript/src/templates/*.ts",
    "**/plugin-*/typescript/src/utils/*.ts",
    "**/plugin-*/typescript/src/tool-compatibility/**",
    "**/plugin-*/typescript/src/config.ts",
    "**/plugin-*/typescript/src/errors.ts",
    "**/plugin-*/typescript/src/provider.ts",
    "**/plugin-*/typescript/src/service.ts",
    "**/plugin-*/typescript/src/tests/**",
    "**/plugin-*/typescript/src/workers/**",
    "**/plugin-*/typescript/types.ts",
    "**/plugin-*/typescript/utils/*.ts",
    "**/plugin-*/typescript/storage-browser.ts",
    "**/plugin-*/typescript/utils/plugin-templates.ts",
    "**/plugin-*/typescript/src/providers/message-classifier.ts",
    "**/plugin-*/typescript/providers/*.ts",
    "**/plugin-*/typescript/services/*.ts",
    "**/plugin-*/typescript/templates/index.ts",
    "**/plugin-*/typescript/types.ts",
    "**/plugin-*/typescript/actions/*.ts",
    "**/plugin-*/typescript/discovery.ts",
    "**/plugin-*/typescript/interactions.ts",
    "**/plugin-*/typescript/post.ts",
    "**/plugin-*/typescript/services/TwitterClient.service.ts",
    "**/plugin-xai/typescript/timeline.ts"
  ],
  "ignoreDependencies": [
    "@types/*",
    "@vitest/*",
    "typescript",
    "prettier",
    "@biomejs/biome",
    "lerna",
    "turbo",
    "vitest",
    "bun"
  ],
  "workspaces": {
    ".": {
      "entry": ["scripts/**/*.ts", "scripts/**/*.js"]
    },
    "packages/*": {
      "entry": ["src/index.ts", "index.ts"]
    },
    "plugins/*/typescript": {
      "entry": ["src/index.ts", "index.ts"]
    },
    "apps/*": {
      "entry": ["src/index.ts", "index.ts"]
    }
  },
  "include": [
    "files",
    "dependencies",
    "unlisted",
    "exports",
    "nsExports",
    "types",
    "nsTypes"
  ]
}
