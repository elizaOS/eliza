{
    "name": "@elizaos/plugin-tee",
    "version": "1.0.0-alpha.0",
    "main": "dist/index.js",
    "type": "module",
    "types": "dist/index.d.ts",
    "dependencies": {
        "@elizaos/core": "workspace:*",
        "@phala/dstack-sdk": "^0.1.7",
        "@solana/web3.js": "^1.98.0",
        "better-sqlite3": "11.8.1",
        "elliptic": "6.6.1"
    },
    "devDependencies": {
        "@biomejs/biome": "1.5.3",
        "@types/node": "^20.0.0",
