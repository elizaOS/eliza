    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.5",
    "tsup": "^8.0.1",
    "typescript": "5.8.3",
    "@types/bun": "1.2.17"
  },
  "keywords": [
    "elizaos",
    "plugin",
    "self-modification",
    "character-evolution",
    "agi"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
