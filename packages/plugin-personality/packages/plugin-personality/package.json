{
  "name": "@elizaos/plugin-personality",
  "version": "1.0.0",
  "description": "ElizaOS plugin for agent self-modification and character evolution",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "test:e2e": "elizaos test --type e2e",
