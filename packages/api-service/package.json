{
  "name": "@elizaos/api-service",
  "version": "0.1.0",
  "description": "ElizaOS API Service - Multi-provider AI inference proxy with cost calculation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run typecheck && node build.ts",
    "dev": "node --watch src/index.ts",
    "start": "node dist/index.js",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:e2e": "cypress run",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "elizaos",
    "api-service",
    "ai-inference",
    "openai",
    "anthropic",
    "gemini",
    "multi-provider"
  ],
  "author": "ElizaOS Team",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.6.3",
    "ai": "^3.4.9",
    "@ai-sdk/openai": "^0.0.66",
    "@ai-sdk/anthropic": "^0.0.46",
    "@ai-sdk/google": "^0.0.54",
    "zod": "^3.23.8",
    "ioredis": "^5.4.1",
    "postgres": "^3.4.5",
    "drizzle-orm": "^0.36.4",
    "drizzle-kit": "^0.28.1",
    "jose": "^5.9.6"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0",
    "eslint": "^9.0.0",
    "@eslint/js": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cypress": "^13.15.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
