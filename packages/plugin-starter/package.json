{
	"name": "@elizaos/plugin-starter",
	"description": "Plugin starter for elizaOS",
	"version": "1.0.0-alpha.22",
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@elizaos/core": "^1.0.0-alpha.22",
		"zod": "3.21.4"
	},
	"devDependencies": {
		"tsup": "8.4.0"
	},
	"scripts": {
		"start": "npx elizaos start",
		"dev": "npx elizaos dev",
		"build": "tsup",
		"lint": "biome check --config-path=./ --write",
		"test": "npx elizaos test",
		"deploy": "npx elizaos deploy"
	},
	"publishConfig": {
		"access": "public"
	},
	"resolutions": {
		"zod": "3.24.1"
	},
	"gitHead": "f2d87b67a335ea97e8a6078536c581f1b2ddc9a0"
}
