{
  "name": "@elizaos/gcp-worker-typescript",
  "version": "1.0.0",
  "description": "elizaOS GCP Cloud Run worker (TypeScript)",
  "type": "module",
  "main": "dist/handler.js",
  "scripts": {
    "build": "esbuild handler.ts --bundle --platform=node --target=node20 --outfile=dist/handler.js --format=esm --banner:js=\"import { createRequire } from 'module'; const require = createRequire(import.meta.url);\"",
    "build:watch": "npm run build -- --watch",
    "dev": "tsx handler.ts",
    "start": "node dist/handler.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "docker:build": "docker build -t eliza-worker-ts .",
    "docker:run": "docker run -p 8080:8080 -e OPENAI_API_KEY=$OPENAI_API_KEY eliza-worker-ts"
  },
  "dependencies": {
    "@elizaos/core": "workspace:*",
    "@elizaos/plugin-openai": "workspace:*",
    "@elizaos/plugin-sql": "workspace:*",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/uuid": "^10.0.0",
    "esbuild": "^0.24.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}

