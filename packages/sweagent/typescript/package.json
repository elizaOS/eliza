{
  "name": "@elizaos/sweagent",
  "version": "1.1.0",
  "description": "SWE-agent: Software Engineering Agent built in TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "sweagent": "./dist/run/cli.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "tools/bin/",
    "tools/dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "bun run build.ts && cd tools && bun run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "bunx @biomejs/biome check --write .",
    "lint:check": "bunx @biomejs/biome check .",
    "format": "bunx @biomejs/biome format --write .",
    "format:check": "bunx @biomejs/biome format .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "dev": "ts-node src/index.ts",
    "rules": "ts-node src/rules/cli.ts",
    "rules:validate": "ts-node src/rules/cli.ts validate",
    "rules:export": "ts-node src/rules/cli.ts export",
    "rules:info": "ts-node src/rules/cli.ts info"
  },
  "keywords": [
    "software-engineering",
    "agent",
    "ai",
    "automation",
    "development-tools",
    "swe-agent",
    "elizaos"
  ],
  "author": "ElizaOS",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "axios": "^1.11.0",
    "chalk": "^5.6.0",
    "commander": "^11.1.0",
    "diff": "^5.2.0",
    "dockerode": "^4.0.7",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "glob": "^10.4.5",
    "inquirer": "^9.3.7",
    "js-yaml": "^4.1.0",
    "jsonl": "^1.1.2",
    "minimatch": "^9.0.5",
    "ora": "^7.0.1",
    "pino": "^8.21.0",
    "pino-pretty": "^10.3.1",
    "playwright": "^1.55.0",
    "sharp": "^0.33.5",
    "simple-git": "^3.28.0",
    "strip-ansi": "^7.1.0",
    "tar-stream": "^3.1.7",
    "wrap-ansi": "^8.1.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/diff": "^5.2.3",
    "@types/dockerode": "^3.3.43",
    "@types/express": "^4.17.23",
    "@types/inquirer": "^9.0.9",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.11",
    "@types/tar-stream": "^3.1.4",
    "@types/ws": "^8.18.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.2",
    "vitest": "^4.0.17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos/eliza"
  },
  "bugs": {
    "url": "https://github.com/elizaos/eliza/issues"
  },
  "homepage": "https://github.com/elizaos/eliza/tree/main/packages/sweagent#readme",
  "publishConfig": {
    "access": "public"
  }
}
