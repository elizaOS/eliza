#!/usr/bin/env node

// This is a wrapper script for the sweagent CLI
// It delegates to the actual CLI implementation in dist/run/cli.js

import("../dist/run/cli.js").catch((err) => {
  const msg = err instanceof Error ? err.message : String(err);
  process.stderr.write(`Failed to start sweagent CLI: ${msg}\n`);
  process.exitCode = 1;
});
