{
	"name": "@elizaos/project-starter",
	"description": "Project starter for elizaOS",
	"version": "1.0.0-alpha.27",
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@elizaos/cli": "1.0.0-alpha.27",
		"@elizaos/core": "1.0.0-alpha.27",
		"zod": "3.21.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"tsup": "8.4.0"
	},
	"scripts": {
		"start": "npx elizaos start",
		"dev": "npx elizaos dev",
		"build": "tsup",
		"lint": "biome check --config-path=./ --write",
		"test": "npx elizaos test",
		"deploy": "npx elizaos deploy"
	},
	"publishConfig": {
		"access": "public"
	},
	"gitHead": "4396652e4e2e5b24c55e23c3b88a7446bbe003b1"
