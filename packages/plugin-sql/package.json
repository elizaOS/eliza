{
	"name": "@elizaos/plugin-sql",
	"version": "1.0.0-alpha.13",
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"@elizaos/source": "./src/index.ts",
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@electric-sql/pglite": "^0.2.17",
		"@elizaos/core": "^1.0.0-alpha.13",
		"@types/pg": "8.11.10",
		"drizzle-kit": "^0.30.4",
		"drizzle-orm": "^0.39.1",
		"pg": "^8.13.3"
	},
	"devDependencies": {
		"dockerode": "^4.0.4",
		"tsup": "8.4.0"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"migrate:generate": "drizzle-kit generate:pg",
		"migrate": "tsup src/migrate.ts --format esm --clean=false --onSuccess=\"node dist/migrate.js\"",
		"lint": "biome check ./src --config-path=./ --apply-unsafe && biome format ./ --config-path=./ --write"
	},
	"peerDependencies": {
		"typescript": "5.8.2"
	},
	"gitHead": "45175c735affb7ea45b3d9efecde34223e782b79"
}
