{
  "name": "@elizaos/core",
  "version": "2.0.0-alpha.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist/browser/index.browser.js",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "browser": {
        "types": "./dist/browser/index.d.ts",
        "import": "./dist/browser/index.browser.js",
        "default": "./dist/browser/index.browser.js"
      },
      "node": {
        "types": "./dist/node/index.d.ts",
        "import": "./dist/node/index.node.js",
        "default": "./dist/node/index.node.js"
      },
      "bun": {
        "types": "./dist/node/index.d.ts",
        "default": "./dist/node/index.node.js"
      },
      "default": "./dist/node/index.node.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.node.js",
      "default": "./dist/node/index.node.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.browser.js",
      "default": "./dist/browser/index.browser.js"
    },
    "./rust": {
      "browser": {
        "import": "./rust/pkg/web/elizaos.js",
        "default": "./rust/pkg/web/elizaos.js"
      },
      "node": {
        "import": "./rust/pkg/node/elizaos.js",
        "default": "./rust/pkg/node/elizaos.js"
      },
      "default": "./rust/pkg/node/elizaos.js"
    },
    "./testing": {
      "import": "./dist/testing/index.js",
      "default": "./dist/testing/index.js"
    }
  },
  "files": [
    "dist",
    "rust/pkg"
  ],
  "scripts": {
    "build": "bun run build:ts",
    "build:all": "bun run build:ts && bun run build:rust",
    "build:watch": "bun run build.ts --watch",
    "dev": "bun run build:watch",
    "clean": "rm -rf dist .turbo node_modules .turbo-tsconfig.json *.tsbuildinfo rust/target rust/pkg python/.venv python/**/__pycache__",

    "test": "bun run test:ts",
    "test:all": "bun run test:ts && bun run test:rust && bun run test:python",

    "lint": "bun run lint:ts",
    "lint:all": "bun run lint:ts && bun run lint:rust && bun run lint:python",
    "lint:check": "bun run lint:ts:check",
    "lint:fix": "bun run lint:ts:fix",

    "typecheck": "bun run typecheck:ts",
    "typecheck:all": "bun run typecheck:ts && bun run typecheck:rust && bun run typecheck:python",

    "format": "bun run format:ts",
    "format:all": "bun run format:ts && bun run format:python",
    "format:check": "bun run format:ts:check",

    "build:ts": "bun run build.ts",
    "build:ts:node": "bun run build.ts --target node",
    "build:ts:browser": "bun run build.ts --target browser",
    "test:ts": "bun test --coverage",
    "test:ts:coverage": "bun test --coverage",
    "test:ts:watch": "bun test --watch",
    "format:ts": "prettier --write ./typescript/src",
    "format:ts:check": "prettier --check ./typescript/src",
    "lint:ts": "eslint typescript/src/ && prettier --write ./typescript/src",
    "lint:ts:check": "eslint typescript/src/",
    "lint:ts:fix": "eslint typescript/src/ --fix && prettier --write ./typescript/src",
    "typecheck:ts": "tsc --noEmit -p ./tsconfig.json",

    "build:rust": "cd rust && ./build.sh",
    "build:rust:native": "cd rust && cargo build --release",
    "build:rust:wasm": "cd rust && wasm-pack build --target web --out-dir pkg/web --features wasm && wasm-pack build --target nodejs --out-dir pkg/node --features wasm",
    "test:rust": "cd rust && cargo test",
    "lint:rust": "cd rust && cargo clippy --all-targets --features native -- -D warnings",
    "lint:rust:fix": "cd rust && cargo clippy --all-targets --all-features --fix --allow-dirty",
    "typecheck:rust": "cd rust && cargo check --all-targets --features native",

    "build:python": "cd python && pip install -e .",
    "test:python": "cd python && pytest",
    "lint:python": "cd python && ruff check . && ruff format --check .",
    "lint:python:fix": "cd python && ruff check --fix . && ruff format .",
    "format:python": "cd python && ruff format .",
    "format:python:check": "cd python && ruff format --check .",
    "typecheck:python": "cd python && python3 -m mypy elizaos || echo 'mypy not installed, skipping typecheck'",

    "build:docs": "cd docs && bun run build"
  },
  "author": "elizaOS",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/fast-redact": "^3.0.4",
    "@types/node": "^25.0.3",
    "@types/uuid": "^11.0.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@langchain/core": "^1.1.12",
    "@langchain/textsplitters": "^1.0.0",
    "adze": "^2.2.5",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^17.2.3",
    "fast-redact": "^3.5.0",
    "glob": "^13.0.0",
    "handlebars": "^4.7.8",
    "pdfjs-dist": "^5.2.133",
    "unique-names-generator": "^4.7.1",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "255e37c0e4a76da0b776219db5ebb9dadf20e89f"
}
