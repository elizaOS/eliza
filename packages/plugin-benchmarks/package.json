{
  "name": "@elizaos/plugin-benchmarks",
  "version": "1.4.0",
  "description": "Action calling benchmark plugins for ElizaOS (Typewriter Aâ€“Z actions)",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "elizaos",
    "plugin",
    "benchmark",
    "typewriter",
    "actions"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "elizaOS Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos/eliza.git",
    "directory": "plugin-action-bench"
  },
  "bugs": {
    "url": "https://github.com/elizaos/eliza/issues"
  },
  "homepage": "https://github.com/elizaos/eliza#readme",
  "dependencies": {
    "@elizaos/core": "^1.4.2"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
