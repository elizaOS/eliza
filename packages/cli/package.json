{
	"name": "@elizaos/cli",
	"version": "1.0.0-alpha.27",
	"description": "elizaOS CLI - Manage your AI agents and plugins",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"author": {
		"name": "elizaOS",
		"url": "https://twitter.com/eliza_OS"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/elizaOS/eliza.git",
		"directory": "packages/cli"
	},
	"files": [
		"dist",
		"templates"
	],
	"keywords": [],
	"type": "module",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"elizaos": "./dist/index.js"
	},
	"scripts": {
		"cli": "bun run build && node dist/index.js",
		"build": "tsup && bun run src/scripts/copy-client-dist.ts && bun run src/scripts/copy-to-create-eliza.ts && bun run src/scripts/copy-templates.ts",
		"release": "changeset version",
		"test:dev": "REGISTRY_URL=http://localhost:3333 vitest run",
		"lint": "biome check ./src --config-path=./ --apply-unsafe && biome format ./ --config-path=./ --write",
		"clean": "rm -rf dist .turbo node_modules .turbo-tsconfig.json tsconfig.tsbuildinfo"
	},
	"dependencies": {
		"@babel/core": "^7.22.1",
		"@babel/parser": "^7.22.6",
		"@elizaos/core": "^1.0.0-alpha.27",
		"@elizaos/plugin-sql": "^1.0.0-alpha.27",
		"axios": "^1.7.9",
		"chalk": "5.2.0",
		"commander": "^10.0.0",
		"diff": "^5.1.0",
		"dotenv": "^16.4.5",
		"execa": "^7.0.0",
		"fs-extra": "^11.1.0",
		"glob": "^11.0.0",
		"https-proxy-agent": "^6.2.0",
		"prompts": "^2.4.2",
		"recast": "^0.23.2",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.19.2",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@types/babel__core": "^7.20.1",
		"@types/diff": "^5.0.3",
		"@types/fs-extra": "^11.0.1",
		"@types/lodash": "^4.17.7",
		"@types/prompts": "^2.4.2",
		"rimraf": "6.0.1",
		"tsup": "8.4.0",
		"type-fest": "^3.8.0",
		"typescript": "5.8.2"
	},
	"peerDependencies": {
		"form-data": "4.0.2",
		"glob": "^11.0.0"
	},
