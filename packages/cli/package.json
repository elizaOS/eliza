{
  "name": "@elizaos/cli",
  "version": "1.0.12",
  "description": "elizaOS CLI - Manage your AI agents and plugins",
  "publishConfig": {
    "access": "public",
    "workspaces": {
      "preserveIfPresent": true
    }
  },
  "license": "MIT",
  "author": {
    "name": "elizaOS",
    "url": "https://twitter.com/eliza_OS"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaOS/eliza.git",
    "directory": "packages/cli"
  },
  "files": [
    "dist",
    "templates"
  ],
  "keywords": [],
  "type": "module",
  "main": "./dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "elizaos": "./dist/src/index.js"
  },
  "scripts": {
    "dev": "bun run build.ts --watch",
    "start": "NODE_OPTIONS='--max-old-space-size=4096' bun run build && node dist/src/index.js start",
    "build": "bun run src/scripts/copy-templates.ts && bun x bun run build.ts",
    "lint": "eslint src --fix && prettier --write ./src",
    "lint:check": "eslint src && prettier --check ./src",
    "typecheck": "NODE_OPTIONS='--max-old-space-size=4096' tsc --noEmit",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "test": "bun test",
    "test:unit": "bun test",
    "test:unit:watch": "bun test --watch",
    "test:unit:coverage": "bun test --coverage",
    "test:cli": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" bun test tests/commands/ --timeout 120000",
    "test:scenarios": "bun run scripts/test-scenarios.ts",
    "test:bats": "bats tests/bats",
    "test:auth-flow": "./scripts/test-auth-flow.sh",
    "coverage": "c8 --reporter=lcov --reporter=text bun test",
    "coverage:html": "c8 --reporter=html bun test && open coverage/index.html"
  },
  "devDependencies": {
    "@types/bun": "^1.2.17",
    "@types/express": "^5.0.2",
    "@types/fs-extra": "^11.0.1",
    "@types/helmet": "^4.0.0",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^24.0.3",
    "@types/prompts": "^2.4.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "bats-assert": "^2.0.0",
    "bats-support": "^0.3.0",
    "cross-env": "^7.0.3",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^9.22.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "vite": "^6.3.5"
  },
  "gitHead": "d5bd5c43bfebeb7ac02f9e029f924cb6cd5c2ec7",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@clack/prompts": "^0.11.0",
    "@elizaos/client": "workspace:*",
    "@elizaos/core": "workspace:*",
    "@elizaos/plugin-openai": "1.0.4",
    "@elizaos/plugin-sql": "workspace:*",
    "@elizaos/server": "workspace:*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "bun": "^1.2.17",
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "dotenv": "^16.5.0",
    "execa": "^9.6.0",
    "fs-extra": "^11.1.0",
    "globby": "^14.0.2",
    "https-proxy-agent": "^7.0.6",
    "inquirer": "^12.6.3",
    "keytar": "^7.9.0",
    "open": "^10.1.0",
    "ora": "^8.1.1",
    "rimraf": "6.0.1",
    "semver": "^7.7.2",
    "simple-git": "^3.27.0",
    "tiktoken": "^1.0.18",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.41.0",
    "yoctocolors": "^2.1.1",
    "zod": "^3.25.67"
  }
}
