{
  "name": "@elizaos/plugin-plugin-manager",
  "description": "Plugin manager for ElizaOS",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageType": "plugin",
  "platform": "node",
  "license": "MIT",
  "author": "ElizaOS",
  "keywords": [
    "plugin",
    "elizaos"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos-plugins/plugin-autonomy"
  },
  "homepage": "https://elizaos.ai",
  "bugs": {
    "url": "https://github.com/elizaos-plugins/plugin-autonomy/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    ".npmignore",
    "package.json",
    "tsup.config.ts"
  ],
  "dependencies": {
    "@elizaos/core": "workspace:*",
    "@elizaos/plugin-github": "workspace:*",
    "@elizaos/plugin-anthropic": "1.0.3",
    "axios": "^1.7.9",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "semver": "^7.6.0",
    "uuid": "^9.0.1",
    "zod": "3.25.67"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@elizaos/cli": "^1.0.0",
    "@eslint/js": "^9.18.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "dotenv": "16.4.5",
    "eslint": "^9.18.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3"
  },
  "scripts": {
    "start": "elizaos start",
    "dev": "elizaos dev",
    "build": "npm run typecheck && tsup src/index.ts --format esm --dts --external @elizaos/core",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "lint:format": "prettier --write ./src",
    "test:component": "bun test src/tests",
    "test:e2e": "bun test src/e2e",
    "test": "elizaos test",
    "test:all": "bun test",
    "coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "publish": "elizaos publish",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "zod": "3.25.67"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "API_KEY": {
        "type": "string",
        "description": "API key for the service"
      }
    }
  },
  "gitHead": "d5bd5c43bfebeb7ac02f9e029f924cb6cd5c2ec7"
}
