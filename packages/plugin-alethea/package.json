{
  "name": "@elizaos/plugin-alethea",
  "description": "ElizaOS plugin for Alethea AI integration",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "alethea",
    "ai",
    "elizaos",
    "plugin"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizaos/cli": "workspace:*",
    "@elizaos/core": "workspace:*",
    "@snapshot-labs/snapshot.js": "^0.8.2",
    "ethers": "^6.14.0",
    "zod": "3.25.32"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@vitest/coverage-v8": "2.1.5",
    "jest": "^29.7.0",
    "prettier": "3.5.3",
    "tsup": "8.4.0",
    "typescript": "5.8.2",
    "vitest": "2.1.5"
  },
  "scripts": {
    "start": "elizaos start",
    "dev": "elizaos dev",
    "build": "tsup",
    "lint": "prettier --write ./src",
    "test:component": "vitest run",
    "test:e2e": "elizaos test",
    "test": "npm run test:component && npm run test:e2e",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageType": "plugin",
  "npmPackage": "@elizaos/plugin-alethea"
}
