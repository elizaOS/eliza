{
  "name": "@elizaos-plugins/plugin-trader",
  "version": "1.0.0-beta.34",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@elizaos/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.3.5"
  },
  "scripts": {
    "build": "tsup --format esm --dts",
    "dev": "tsup --format esm --dts --watch",
    "clean": "rm -rf dist",
