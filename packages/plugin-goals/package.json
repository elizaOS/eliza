{
  "name": "@elizaos/plugin-goals",
  "description": "Todo list plugin for ElizaOS",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageType": "plugin",
  "platform": "node",
  "license": "UNLICENSED",
  "author": "ElizaOS",
  "keywords": [
    "plugin",
    "elizaos"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos-plugins/plugin-autonomy"
  },
  "homepage": "https://elizaos.ai",
  "bugs": {
    "url": "https://github.com/elizaos-plugins/plugin-autonomy/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    ".npmignore",
    "package.json",
    "tsup.config.ts"
  ],
  "dependencies": {
    "@elizaos/core": "workspace:*",
    "drizzle-orm": "^0.44.2",
    "start-server-and-test": "^2.0.5",
    "uuid": "^11.0.3",
    "zod": "3.24.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@elizaos/cli": "^1.0.0",
    "@eslint/js": "^9.22.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "cypress": "^13.6.3",
    "dotenv": "16.4.5",
    "drizzle-kit": "^0.30.1",
    "eslint": "^9.22.0",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "5.8.3"
  },
  "scripts": {
    "start": "elizaos start",
    "dev": "elizaos dev",
    "build": "tsup",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "test": "elizaos test",
    "test:skip-tsc": "node scripts/test-without-tsc.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:test": "cypress run --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e": "node scripts/test-e2e.js",
    "test:e2e:headless": "CYPRESS_HEADED=false node scripts/test-e2e.js",
    "test:e2e:ci": "CI=true node scripts/test-e2e-ci.js",
    "publish": "elizaos publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "zod": "3.24.2"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "API_KEY": {
        "type": "string",
        "description": "API key for the service"
      }
    }
  },
  "gitHead": "d5bd5c43bfebeb7ac02f9e029f924cb6cd5c2ec7"
}
