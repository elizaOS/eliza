{
	"name": "@elizaos/the-org",
	"version": "1.0.0-alpha.6",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"start": "bun run build && npx elizaos start",
		"build": "tsup --format esm --dts",
		"dev": "node --loader ts-node/esm src/index.ts",
		"check-types": "tsc --noEmit",
		"test": "vitest --run"
	},
	"nodemonConfig": {
		"watch": [
			"src",
			"../core/dist"
		],
		"ext": "ts,json",
		"exec": "node --enable-source-maps --loader ts-node/esm src/index.ts"
	},
	"dependencies": {
		"@elizaos/cli": "workspace:*",
		"@elizaos/core": "workspace:*",
		"@elizaos/plugin-tee": "workspace:*",
		"@elizaos/plugin-twitter": "workspace:*",
		"@solana/web3.js": "^1.87.6",
		"@types/body-parser": "1.19.5",
		"@types/cors": "2.8.17",
		"@types/express": "5.0.0",
		"cors": "2.8.5",
		"express": "4.21.1",
		"minipass": "7.1.2",
		"multer": "1.4.5-lts.1",
		"readline": "1.3.0",
		"ts-node": "^10.9.2",
		"typescript": "5.8.2",
		"ws": "8.18.0",
		"yargs": "17.7.2",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@types/multer": "^1.4.12",
		"ts-node": "10.9.2",
		"tsup": "8.4.0"
	},
	"gitHead": "084c3b06c279b85528a81d42fc93ef68ff9f7364"
}
