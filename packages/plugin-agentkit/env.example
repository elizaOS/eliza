# CDP API Credentials
# Get these from https://portal.cdp.coinbase.com/
CDP_API_KEY_NAME=your-api-key-id-here
CDP_API_KEY_PRIVATE_KEY=your-private-key-here
CDP_NETWORK_ID=base-sepolia

# Optional: Client API Key
CDP_CLIENT_API_KEY=your-client-api-key-here

# Encryption passphrase for custodial wallets
CUSTODIAL_WALLET_ENCRYPTION_PASSPHRASE=your-secure-passphrase-here

# Trust Score Integration (optional)
ENABLE_TRUST_SCORING=false
MIN_TRUST_SCORE_FOR_TRANSACTIONS=50 