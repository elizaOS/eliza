{
  "name": "@elizaos/plugin-agentkit",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@coinbase/agentkit": "^0.8.2",
    "@coinbase/cdp-sdk": "^1.16.0",
    "@elizaos/core": "workspace:*",
    "@elizaos/plugin-trust": "workspace:*"
  },
  "devDependencies": {
    "@types/cypress": "^1.1.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "cypress": "^13.0.0",
    "eslint": "^9.22.0",
    "prettier": "3.5.3"
  },
  "scripts": {
    "build": "bun run build.ts",
    "dev": "bun run build.ts --watch",
    "clean": "rm -rf dist && rm -rf .turbo",
    "lint": "eslint src --fix && prettier --write ./src",
    "typecheck": "tsc --noEmit",
    "test": "bun test src/"
  },
  "publishConfig": {
    "access": "public"
  },
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "CDP_API_KEY_NAME": {
        "type": "string",
        "minLength": 1,
        "description": "your_key_name",
        "optional": false
      },
      "CDP_API_KEY_PRIVATE_KEY": {
        "type": "string",
        "minLength": 1,
        "description": "your_private_key",
        "optional": false
      },
      "CDP_AGENT_KIT_NETWORK": {
        "type": "string",
        "minLength": 1,
        "description": "Optional: Defaults to base-sepolia",
        "optional": true
      }
    }
  }
}
