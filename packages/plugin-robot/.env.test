# Test environment configuration for robot plugin

# Use mock robot for testing
USE_MOCK_ROBOT=true
USE_SIMULATION=false

# Mock adapter settings
MOCK_SIMULATE_DELAY=true
MOCK_DEFAULT_DELAY=50
MOCK_FAILURE_RATE=0

# Test mode indicator
NODE_ENV=test
ELIZA_TEST=true

# Vision settings (for vision tests)
VISION_PROVIDER=mock
MOCK_VISION_DELAY=100

# Logging
LOG_LEVEL=debug
