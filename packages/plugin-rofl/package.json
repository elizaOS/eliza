{
  "name": "plugin-rofl",
  "description": "Rofl plugin for ElizaOS",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageType": "plugin",
  "platform": "node",
  "license": "UNLICENSED",
  "author": "elizaos",
  "keywords": [
    "plugin",
    "elizaos"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monilpat/plugin-rofl.git"
  },
  "homepage": "https://elizaos.ai",
  "bugs": {
    "url": "https://github.com/elizaos/eliza/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    ".npmignore",
    "package.json",
    "tsup.config.ts"
  ],
  "dependencies": {
    "@elizaos/cli": "workspace:*",
    "@elizaos/core": "workspace:*",
    "whatwg-url": "7.1.0",
    "axios": "^1.9.0"
  },
  "devDependencies": {
    "tsup": "8.5.0",
    "typescript": "5.8.2",
    "dotenv": "16.4.5",
    "prettier": "3.5.3",
    "@types/node": "^20.0.0"
  },
  "peerDependencies": {},
  "scripts": {
    "start": "elizaos start",
    "dev": "elizaos dev",
    "build": "tsc --noEmit && tsup",
    "test": "vitest run",
    "lint": "prettier --write ./src",
    "publish": "elizaos publish",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "zod": "3.25.32"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "API_KEY": {
        "type": "string",
        "description": "API key for the service"
      }
    }
  },
  "gitHead": "d5bd5c43bfebeb7ac02f9e029f924cb6cd5c2ec7",
  "npmPackage": "plugin-rofl"
}