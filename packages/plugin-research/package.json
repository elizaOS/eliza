{
  "name": "@elizaos/plugin-research",
  "version": "0.1.0",
  "description": "Deep research plugin for ElizaOS with multi-phase internet research capabilities",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun run build.ts",
    "dev": "bun run build.ts --watch",
    "clean": "rm -rf dist && rm -rf .turbo",
    "lint": "eslint src --fix && prettier --write ./src",
    "typecheck": "tsc --noEmit",
    "test": "bun test src/",
    "benchmark": "node scripts/run-benchmarks.ts",
    "benchmark:deepresearch": "node scripts/run-benchmarks.ts deepresearch",
    "benchmark:breadth": "node scripts/run-benchmarks.ts breadth",
    "benchmark:speed": "node scripts/run-benchmarks.ts speed",
    "benchmark:accuracy": "node scripts/run-benchmarks.ts accuracy",
    "benchmark:comprehensive": "node scripts/run-benchmarks.ts comprehensive",
    "benchmark:cli": "node src/benchmarks/benchmark-cli.ts"
  },
  "dependencies": {
    "@elizaos/core": "workspace:*",
    "@elizaos/plugin-stagehand": "workspace:*",
    "@types/pdf-parse": "^1.1.1",
    "axios": "^1.7.2",
    "cheerio": "^1.0.0",
    "dotenv": "^16.5.0",
    "duck-duck-scrape": "^2.2.7",
    "lru-cache": "^10.2.0",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.45.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@browserbasehq/stagehand": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@browserbasehq/stagehand": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.22.0",
    "prettier": "^3.2.2",
    "typescript": "5.8.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "elizaos",
    "ai",
    "research",
    "plugin",
    "web-search",
    "content-extraction",
    "deep-research",
    "defi"
  ],
  "author": "ElizaOS Contributors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos/eliza.git"
  },
  "bugs": {
    "url": "https://github.com/elizaos/eliza/issues"
  },
  "homepage": "https://github.com/elizaos/eliza#readme",
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {}
  }
}
