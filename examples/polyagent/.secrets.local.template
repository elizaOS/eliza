# Local secrets for act (GitHub Actions runner)
# Copy this file to .secrets.local and fill in your values
# NEVER commit .secrets.local to git!

# Database (use default CI values - act starts postgres service container)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/test_db
DIRECT_DATABASE_URL=postgresql://postgres:postgres@localhost:5432/test_db

# Privy Authentication (required for auth-related tests)
PRIVY_APP_ID=
PRIVY_APP_SECRET=
NEXT_PUBLIC_PRIVY_APP_ID=

# Privy Test Credentials (for E2E tests)
PRIVY_TEST_EMAIL=
PRIVY_TEST_PASSWORD=
PRIVY_TEST_PHONE=
PRIVY_TEST_OTP=

# AI API Keys (at least one required for AI tests)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GROQ_API_KEY=

# Other Services
WANDB_API_KEY=
FAL_KEY=
CRON_SECRET=test-cron-secret

# Wallet (for blockchain tests)
WALLET_SEED_PHRASE=
WALLET_PASSWORD=

# Redis (act starts redis service container)
REDIS_URL=redis://localhost:6379

# Test User Credentials (optional, for authenticated API tests)
TEST_USER_ID=
TEST_ACCESS_TOKEN=

# Agent0 Configuration (optional)
AGENT0_ENABLED=false
AGENT0_NETWORK=sepolia
AGENT0_RPC_URL=
AGENT0_PRIVATE_KEY=
AGENT0_IPFS_PROVIDER=pinata
PINATA_JWT=
AGENT0_SUBGRAPH_URL=












