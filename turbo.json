{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", "build/**"]
      },
      "start": {
        "dependsOn": ["build", "^start"],
        "persistent": true
      },
      "dev": {
        "persistent": true,
        "dependsOn": [],
        "cache": false
      },
      "test": {
        "persistent": true,
        "dependsOn": ["build"]
      },
      "lint:fix": {
        "dependsOn": ["build"]
      },
      "swarm": {
        "dependsOn": ["build"]
      }
    }
}
