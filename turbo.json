{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": ["LOG_LEVEL"],
      "outputs": ["dist/**", "build/**"],
      "cache": false
    },
    "start": {
      "dependsOn": ["^start"],
      "env": ["LOG_LEVEL"],
      "persistent": true,
      "cache": false
    },
    "dev": {
      "persistent": true,
      "dependsOn": ["^dev"],
      "cache": false
    },
    "test": {
      "persistent": true,
      "dependsOn": ["^test"]
    },
    "lint:fix": {
      "dependsOn": ["^lint:fix"]
    },
    "swarm": {
      "dependsOn": ["^swarm"]
    },
    "migrate": {
      "dependsOn": ["^migrate"]
    },
    "migrate:generate": {
      "dependsOn": ["^migrate:generate"]
    }
  }
}
