# XMRT-Eliza Environment Configuration
# Copy this file to .env and fill in your values

# Core Eliza Configuration
NODE_ENV=development
LOG_LEVEL=info

# XMRT Ecosystem Configuration
XMRT_SUPABASE_URL=https://vawouugtzwmejxqkeqqj.supabase.co
XMRT_SUPABASE_KEY=your_supabase_publishable_key_here
XMRT_ECOSYSTEM_API_URL=https://xmrt-ecosystem.vercel.app
XMRT_SUITE_AI_URL=https://suite.lovable.app

# Agent Configuration
XMRT_AGENT_ID=primary-eliza
XMRT_COUNCIL_MODE=true
XMRT_ENABLE_REALTIME=true

# AI Model Configuration
ANTHROPIC_API_KEY=your_anthropic_key_here
TOGETHER_API_KEY=your_together_key_here

# Optional: Additional XMRT Services
XMRT_GITHUB_TOKEN=your_github_token_here
XMRT_WEBHOOK_SECRET=your_webhook_secret_here

# Communication Settings
XMRT_HEARTBEAT_INTERVAL=60000
XMRT_MAX_RETRY_ATTEMPTS=3
XMRT_CONNECTION_TIMEOUT=30000

# Logging and Monitoring
XMRT_ENABLE_DETAILED_LOGGING=true
XMRT_LOG_COORDINATION_EVENTS=true
XMRT_REPORT_INTERVAL=3600000
